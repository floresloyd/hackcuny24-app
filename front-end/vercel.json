{
  "version": 2,
  "routes": [
    { "source": "/login", "destination": "/" },
    { "source": "/register", "destination": "/" },
    { "source": "/forgot", "destination": "/" },
    { "source": "/", "destination": "/" },
    { "source": "/events", "destination": "/" },
    { "source": "/leaderboard", "destination": "/" },
    { "source": "/analysis", "destination": "/" },
    {
      "source": "/(.*)",
      "destination": "/",
      "permanent": false
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ]
}
